<FilesMatch "\\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
    Deny from all
</FilesMatch>
  

RewriteEngine On

RewriteRule ^zatrudnij-programiste-webowego$ index.html [L]
RewriteRule ^kim-jestem$ kimjestem.html [L]
RewriteRule ^oferta-tworzenie-stron-www$ oferta.html [L]
RewriteRule ^cv-programista-php-javascript$ cv.html [L]
RewriteRule ^kontakt-programista-php$ kontakt.html [L]